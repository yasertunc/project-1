                "85%": {
                  "scale": 0.8,
                  "rotation": "4deg",
                  "blur": "1px",
                  "brightness": 1.1
                },
                "100%": {
                  "scale": 0.1,
                  "rotation": "5deg",
                  "blur": "5px",
                  "brightness": 2,
                  "opacity": 0

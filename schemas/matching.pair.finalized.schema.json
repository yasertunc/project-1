{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://fellowus.com/schemas/matching.pair.finalized.schema.json",
  "title": "matching.pair.finalized",
  "type": "object",
  "allOf": [
    {
      "$ref": "https://fellowus.com/schemas/common.json"
    }
  ],
  "properties": {
    "eventName": {
      "const": "matching.pair.finalized"
    },
    "eventVersion": {
      "type": "string",
      "pattern": "^[0-9]+\\.[0-9]+$"
    },
    "eventTime": {
      "type": "string",
      "format": "date-time"
    },
    "traceId": {
      "type": "string"
    },
    "requestId": {
      "type": ["string", "null"]
    },
    "matchId": {
      "type": "string"
    },
    "channelId": {
      "type": ["string", "null"]
    },
    "channelType": {
      "type": ["string", "null"],
      "enum": ["dm", "group", null]
    },
    "userId": {
      "type": ["string", "null"]
    },
    "participants": {
      "type": ["array", "null"],
      "items": {
        "type": "string"
      }
    },
    "intent": {
      "type": ["string", "null"],
      "enum": ["incident", "route", "social", "coach", null]
    },
    "platform": {
      "type": ["string", "null"],
      "enum": ["ios", "android", "web", null]
    },
    "appVersion": {
      "type": ["string", "null"]
    },
    "locale": {
      "type": ["string", "null"]
    },
    "region": {
      "type": ["string", "null"]
    },
    "privacyTier": {
      "type": ["string", "null"],
      "enum": ["anon", "low", "high", null]
    },
    "status": {
      "type": "string",
      "enum": ["matched", "declined", "timeout", "cancelled", "error"]
    },
    "finalizedAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "traceId",
    "finalizedAt",
    "eventVersion",
    "status",
    "eventName",
    "matchId",
    "eventTime"
  ]
}
